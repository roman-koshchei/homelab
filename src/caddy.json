{
  "apps": {
    "http": {
      "http_port": 80,
      "https_port": 443,
      "servers": {
        "cloud": {
          "listen": [":443"],
          "routes": [
            {
              "match": [{ "host": ["cloud.cookingweb.dev"] }],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": ":5000"
                    }
                  ]
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  },
  "logging": {
    "logs": {
      "default": {
        "level": "ERROR"
      }
    }
  }
}
